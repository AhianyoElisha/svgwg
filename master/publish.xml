<!-- Publication script configuration for SVG 2. -->

<publish-conf xmlns='http://mcc.id.au/ns/local'>
  <title>Scalable Vector Graphics (SVG) 2</title>
  <short-title>SVG 2</short-title>
  <maturity>CR</maturity>
  <output use-publish-directory='true' publish-directory='build/publish'/>

  <!--
    Uncomment the publication date when it is time to publish on
    https://www.w3.org/TR/.  While an Editor's Draft, leave this
    commented out and the last commit date will be used as the
    document's date.
    -->

  <publication-date>2016-08-30</publication-date>

  <versions>
    <cvs href='https://svgwg.org/svg2-draft/'/>
    <cvs-single href='https://svgwg.org/svg2-draft/single-page.html'/>
    <this href='https://www.w3.org/TR/2016/CR-SVG2-20160830/'/>
    <this-single href='https://www.w3.org/TR/2015/CR-SVG2-20160830/single-page.html'/>
    <previous href='https://www.w3.org/TR/2015/WD-SVG2-20150915/'/>
    <latest href='https://www.w3.org/TR/SVG2/'/>
    <latestrec href='https://www.w3.org/TR/SVG/'/>
  </versions>

  <definitions href='definitions.xml'/>
  <definitions href='definitions-filters.xml' base='https://dev.w3.org/fxtf/filters/'/>
  <definitions href='definitions-masking.xml' base='https://dev.w3.org/fxtf/css-masking-1/'/>
  <definitions href='definitions-compositing.xml' base='https://dev.w3.org/fxtf/compositing-1/'/>
  <definitions href='../specs/animations/master/definitions.xml' base='https://svgwg.org/specs/animations/'/>

  <interfaces idl='../build/svg.idlx'/>

